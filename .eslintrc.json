{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "eslint-plugin-import"],
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
  },
  "settings": {}
}
